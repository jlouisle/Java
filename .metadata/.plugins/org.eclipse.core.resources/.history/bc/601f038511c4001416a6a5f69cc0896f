package main;

public class Animal {
	private String name;
	private int weight;
	private AnimalType v;
	
	
	public Animal(String name, int weight, AnimalType v) {
		super();
		this.name = name;
		this.weight = weight;
		this.v = v;
	}
	
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((name == null) ? 0 : name.hashCode());
		result = prime * result + ((v == null) ? 0 : v.hashCode());
		result = prime * result + weight;
		return result;
	}

	@Override
	public String toString() {
		return "Animal [name=" + name + ", weight=" + weight + ", v=" + v + "]";
	}

	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getWeight() {
		return weight;
	}
	public void setWeight(int weight) {
		this.weight = weight;
	}
	public AnimalType getV() {
		return v;
	}
	public void setV(AnimalType v) {
		this.v = v;
	}
	
	
	
	

}
