import java.awt.Color;

import javax.swing.*;


public class GuidedUI {
	private JFrame f;
	private JPanel p;
	private JButton b1;
	private JLabel lab;
	
	public GuidedUI() {
		gui();
	}
	
	public void gui(){
		f = new JFrame("F");
		f.setVisible(true);
		f.setSize(600, 400);
		f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		p = new JPanel();
		p.setBackground(Color.cyan);
		
		b1 = new JButton("Click");
		lab = new JLabel("Label");
		
		p.add(b1);
		p.add(lab);
		
		f.add(p);
		
	}
	public static void main(String[] args) {
		new test();
	}
}
